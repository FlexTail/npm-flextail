{
  "name": "@flextail/ui",
  "version": "0.0.9-rc2",
  "description": "A CLI utility to install FlexTail components.",
  "main": "dist/cli.js",
  "type": "module",
  "scripts": {
    "start": "node dist/cli.js",
    "build": "tsup scripts/cli.js --format esm --platform node --out-dir dist",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "flextail": "dist/cli.js"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "cli"
  ],
  "author": "Syed Arbaaz Hussain",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlexTail/npm-flextail.git"
  },
  "bugs": {
    "url": "https://github.com/FlexTail/npm-flextail/issues"
  },
  "homepage": "https://github.com/FlexTail/npm-flextail#readme",
  "dependencies": {
    "commander": "^14.0.2",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "diff": "^8.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
"publishConfig": {
    "access": "public"
  }
}